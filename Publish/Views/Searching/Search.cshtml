
@{
    ViewBag.Title = "Search";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="page-content" style="min-height:1200px">
    <div class="row">
        <div class="col-md-10">
            <form class="search-form search-form-expanded" action=" http://qdesing.tech/Customer/CustomerDetail" method="GET">
                <div class="input-group">
                    <input type="text" class="form-control" placeholder="Customer Search" name="id">
                    <span class="input-group-btn">
                        <a href="javascript:;" class="btn submit">
                            <i class="icon-magnifier"></i>
                        </a>
                    </span>
                </div>
            </form>            
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-md-10">
            <div class="portlet-body">
                <div class="table-responsive">
                    <table class="table table-striped table-bordered table-hover">
                        <thead>
                            <tr>
                                <th width="5">İş Emri </th>
                                <th width="15"> Customer Nu </th>
                                <th width="15"> Bakım yeri </th>
                                <th width="15"> Yapılan İşlem </th>

                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td> @Html.Raw(ViewBag.JID)</td>
                                <td> @Html.Raw(ViewBag.CID)</td>
                                <td>@Html.Raw(ViewBag.WPLACE)</td>
                                <td>@Html.Raw(ViewBag.ACTION)</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

</div>
