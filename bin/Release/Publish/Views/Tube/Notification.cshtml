
@model IEnumerable<FireLow.TUBE>
@{
    ViewBag.Title = "Notification";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
}

<div class="page-content" style="min-height:1112px">
    <div class="portlet-body form">
        <form class="form-horizontal" action="~//" method="post">
            <div class="portlet light bordered">
                <p></p>
                <div class="portlet-body">
                    <div class="alert alert-block alert-danger fade in">
                        <button type="button" class="close" data-dismiss="alert"></button>
                        <h4 class="alert-heading">DİKKAT !</h4>
                        <p> @Html.Raw(ViewBag.TSERIAL)  Seri Numaralı Yangın Tüpünün Kaydı Silinecek Onaylıyor musunuz? </p>

                        <p>  </p>

                        <p>

                            <a class="btn red" href="/Tube/TubeSave?ID=@Html.Raw(ViewBag.ID)"> Sil </a>
                            <a class="btn blue" href="/Tube/TubeTable"> Vazgeç </a>
                        </p>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>
