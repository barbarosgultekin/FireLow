@model IEnumerable<FireLow.CUSTOMER>
@{
    ViewBag.Title = "CustomerDetail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
}
<div class="page-content" style="min-height:1112px">
    <div class="portlet-body form">
        <form class="form-horizontal" action="" method="post">
            <div class="form-body">
                <h3 class="form-section">@Html.Raw(ViewBag.CNAME)</h3>
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="control-label col-md-3">Ünvan</label>
                            <div class="col-md-8">
                                <input type="text" id="" name="" class="form-control" placeholder="-" disabled>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="control-label col-md-3">Firma / Adı Soyadı</label>
                            <div class="col-md-8">
                                <input type="text" id="APPELLATION" name="APPELLATION" class="form-control" value="@Html.Raw(ViewBag.APPELLATION)" disabled>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="control-label col-md-3">Yetkili Adı Soyadı</label>
                            <div class="col-md-8">
                                <input type="text" id="CNAME" name="CNAME" class="form-control" value="@Html.Raw(ViewBag.CNAME)" disabled>
                                
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="control-label col-md-3">Telefon</label>
                            <div class="col-md-8">
                                <input type="text" id="CGSM " name="CGSM" class="form-control" value="@Html.Raw(ViewBag.CGSM)" disabled>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="control-label col-md-3">Vergi Numarası</label>
                            <div class="col-md-8">
                                <input type="text" id="CVNO" name="CVNO" class="form-control" value="@Html.Raw(ViewBag.CVNO)" disabled>
                            </div>
                        </div>
                    </div>

                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="control-label col-md-3">T.C Kimlik Numarası</label>
                            <div class="col-md-8">
                                <input type="text" id="CTCNO" name="CTCNO" class="form-control" value="@Html.Raw(ViewBag.CTCNO)" disabled>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="control-label col-md-3">E-Posta Adresi</label>
                            <div class="col-md-8">
                                <input type="text" id="CMAIL" name="CMAIL" class="form-control" value="@Html.Raw(ViewBag.CMAIL)" disabled>
                            </div>
                        </div>
                    </div>
                </div>
                <h3 class="form-section">Adres Bilgileri</h3>               
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="control-label col-md-3">Adres</label>
                            <div class="col-md-8">
                                <input type="text" id="CADRESS" name="CADRESS" class="form-control" value="@Html.Raw(ViewBag.CADRESS)" disabled>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="control-label col-md-3">Adres 2</label>
                            <div class="col-md-8">
                                <input type="text" id="CADRESS2" name="CADRESS2" class="form-control" value="@Html.Raw(ViewBag.CADRESS2)" disabled>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="control-label col-md-3">Sanayi Bölgesi</label>
                            <div class="col-md-8">
                                <input type="text" id="CINDUSTRIAREA" name="CINDUSTRIAREA" class="form-control" value="@Html.Raw(ViewBag.CINDUSTRIAREA)" disabled>
                            </div>
                        </div>
                    </div>
                    <!--/span-->
                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="control-label col-md-3">No</label>
                            <div class="col-md-8">
                                <input type="text" id="CPROVINCENO" name="CPROVINCENO" class="form-control" value="@Html.Raw(ViewBag.CPROVINCENO)" disabled>
                            </div>
                        </div>
                    </div>
                    <!--/span-->
                </div>
                <!--/row-->
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="control-label col-md-3">İl</label>
                            <div class="col-md-8">
                                <input type="text" id="CPROVINCE" name="CPROVINCE" class="form-control" value="@Html.Raw(ViewBag.CPROVINCE)" disabled>
                            </div>
                        </div>
                    </div>
                    <!--/span-->
                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="control-label col-md-3">İlçe</label>
                            <div class="col-md-8">
                                <input type="text" id="CDISTRICT" name="CDISTRICT" class="form-control" value="@Html.Raw(ViewBag.CDISTRICT)" disabled>
                            </div>
                        </div>
                    </div>                    
                </div>
            </div>
        </form>
    </div>
</div>

